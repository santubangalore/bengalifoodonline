<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <meta name="description" content="">
    <meta name="author" content="santu ghosh">
    <meta name="description" content="Bengali Food, Bengali catering">
    <meta name="keywords" content="Bengali Food, Home Cooked food, Bengali restaurant, Bengali catering near Nagavara, Bengali food near Manyata">
   <title>@ViewBag.Title | Suruchi Foods, Home of bengali foods</title>

    <!-- core CSS -->
   <link rel="stylesheet" href="http://maxcdn.bootstrapcdn.com/bootstrap/3.3.6/css/bootstrap.min.css">
<script src="https://ajax.googleapis.com/ajax/libs/jquery/1.12.0/jquery.min.js"></script>
<script src="http://maxcdn.bootstrapcdn.com/bootstrap/3.3.6/js/bootstrap.min.js"></script>

    
    <link href='//fonts.googleapis.com/css?family=Stint+Ultra+Condensed' rel='stylesheet' type='text/css'>
    <link href='//fonts.googleapis.com/css?family=Roboto:400,100,100italic,300,300italic,400italic,500,500italic,700,700italic,900,900italic' rel='stylesheet' type='text/css'>
    <link href="~/Content/themes/base/jquery-ui.css" rel="stylesheet" />
    <link href="~/Content/themes/base/jquery.ui.all.css" rel="stylesheet" />
    <script src="~/Scripts/jquery-ui-1.8.24.min.js"></script>
    

    <script src="~/Scripts/card.js"></script>
    <script>

        jQuery.browser = {};
        (function () {
            jQuery.browser.msie = false;
            jQuery.browser.version = 0;
            if (navigator.userAgent.match(/MSIE ([0-9]+)\./)) {
                jQuery.browser.msie = true;
                jQuery.browser.version = RegExp.$1;
            }
        })();
    </script>
    <!--[if lt IE 9]>
    <script src="js/html5shiv.js"></script>
    <script src="js/respond.min.js"></script>
      
       
        
    <![endif]-->

     <link rel="shortcut icon" href="~/Images/logo/1477686379_kitchen-78.ico">
   
    <link href="/Content/style.css" type="text/css" rel="stylesheet" media="all">
    <link rel="stylesheet" href="/Content/flexslider.css" type="text/css" media="screen" />
    <!--// css -->
    <link rel="stylesheet" href="/Content/owl.carousel.css" type="text/css" media="all">
    <link href="~/Content/owl.theme.css" rel="stylesheet">
    <!-- font-awesome icons -->
    <link href="/Content/animate.css" rel="stylesheet" type="text/css" media="all">
    <script src="~/Scripts/wow.min.js"></script>
    <script src="~/Scripts/angular.min.js"></script>
    <script src="~/Scripts/angular-route.min.js"></script>
    <script src="~/ViewModel/bengalifoodsmodule.js"></script>
    <script>
        new WOW().init();
    </script>
    <!--//end-animate-->
    <!-- start-smooth-scrolling-->
    <script type="text/javascript" src="/Scripts/move-top.js"></script>
    <script type="text/javascript" src="/Scripts/easing.js"></script>
    <script type="text/javascript">
        jQuery(document).ready(function ($) {
            $(".scroll").click(function (event) {
                event.preventDefault();
                $('html,body').animate({ scrollTop: $(this.hash).offset().top }, 1000);
            });
        });
    </script>
    <script src="~/scripts/circle-progress.js"></script>

    <style>
        .navbar-inverse .navbar-nav > .active {
            color: #000;
            background: #d65c14;
        }

        .navbar-default .navbar-nav > .active > a,
        .navbar-default .navbar-nav > .active > a:hover,
        .navbar-default .navbar-nav > .active > a:focus {
            color: #000;
            background: #d65c14;
        }

        .image {
            position: relative;
            height: 90px;
            width: 90px; /* for IE 6 */
        }

        h2 span {
            color: white;
            font: bold 24px/45px Helvetica, Sans-Serif;
            letter-spacing: -1px;
            background: rgb(0, 0, 0); /* fallback color */
            background: rgba(0, 0, 0, 0.7);
            padding: 10px;
        }
    </style>
    <link rel="shortcut icon" href="images/ico/favicon.ico">
    <link rel="apple-touch-icon-precomposed" sizes="144x144" href="images/ico/apple-touch-icon-144-precomposed.png">
    <link rel="apple-touch-icon-precomposed" sizes="114x114" href="images/ico/apple-touch-icon-114-precomposed.png">
    <link rel="apple-touch-icon-precomposed" sizes="72x72" href="images/ico/apple-touch-icon-72-precomposed.png">
    <link rel="apple-touch-icon-precomposed" href="images/ico/apple-touch-icon-57-precomposed.png">
</head>
<!--/head-->

<body class="homepage">

    <div style="background-color: darkgoldenrod">
        <div class="row">
            <div  class="col-md-3 pull-left">
                &nbsp;
            </div>
            <div  class="col-md-3 ">
               &nbsp;
            </div>
     
            <div  class="col-md-3 pull-right">
                @{ Html.RenderPartial("_LoginPartial");}

            </div>

       
        </div>
        <div class="container">
        
            <div class="header">
               
                <div class="spacer"></div>
                <div class="logo">
                    <h1 class="wow zoomIn animated" data-wow-delay=".5s"><a href="index.html">Suruchi Foods</a></h1>
                </div>
                <div class="top-nav">
                    
                    <ul>
                        <li class="wow slideInDown animated" data-wow-delay=".5s">@Html.ActionLink("Home","Index","home") </li>
                        <li class="wow slideInDown" data-wow-delay=".6s"></li>
                        <li class="wow slideInDown" data-wow-delay=".7s">@Html.ActionLink("Gallery","Gallery","home") </li>
                        <li class="wow slideInDown" data-wow-delay=".7s">@Html.ActionLink("Order","Order","home")</li>
                        <li class="wow slideInDown" data-wow-delay=".8s">@Html.ActionLink("Blog","Blog","home")</li>
                        <li class="wow slideInDown" data-wow-delay=".9s">@Html.ActionLink("Contact","Contact","home")</li>
                    </ul>
                    <!-- script-for-menu -->
                    <script>
                        $("span.menu").click(function () {
                            $(".top-nav ul").slideToggle("slow", function () {
                            });
                        });
                    </script>
                    <!-- script-for-menu -->
                </div>
               
            </div>

        </div>
        <!--/.container-->
    </div>
    <!--/.top-bar-->

    <!--/header-->

    <div ng-app="OnlinefoodstoreApp">

        <div id="body">
            @RenderSection("featured", required: false)
            <section class="content-wrapper main-content clear-fix">
                @RenderBody()
            </section>
        </div>
    </div>

    
    
    <script src="~/Scripts/owl.carousel.js"></script>  
</body>


</html>
